<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.ButtonBar?>
<?import javafx.scene.control.TableColumn?>
<?import javafx.scene.control.TableView?>
<?import javafx.scene.control.TextField?>
<?import javafx.scene.image.Image?>
<?import javafx.scene.image.ImageView?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.layout.BorderPane?>
<?import javafx.scene.layout.VBox?>

<AnchorPane prefHeight="500.0" prefWidth="803.0" stylesheets="@home.css" xmlns="http://javafx.com/javafx/20.0.1" xmlns:fx="http://javafx.com/fxml/1" fx:controller="org.FPAS.javaFXApp.controller.InvestmentEntryController">
    <children>
        <VBox fx:id="sideMenu" layoutY="77.0" prefHeight="423.0" prefWidth="118.0">
            <children>
                <ButtonBar prefHeight="40.0" prefWidth="93.0">
                    <buttons>
                        <Button fx:id="dashboardButton" mnemonicParsing="false" prefHeight="12.0" prefWidth="83.0" styleClass="buttons" text="Dashboard" translateX="-15.0" />
                    </buttons>
                </ButtonBar>
                <ButtonBar prefHeight="40.0" prefWidth="200.0">
                    <buttons>
                        <Button fx:id="transactionButton" mnemonicParsing="false" prefHeight="25.0" prefWidth="83.0" styleClass="buttons" text="Transactions" translateX="-15.0" />
                    </buttons>
                </ButtonBar>
                <ButtonBar prefHeight="40.0" prefWidth="200.0">
                    <buttons>
                        <Button fx:id="investmentsButton" mnemonicParsing="false" prefHeight="25.0" prefWidth="83.0" styleClass="buttons" text="Investments" translateX="-15.0" />
                    </buttons>
                </ButtonBar>
                <AnchorPane prefHeight="305.0" prefWidth="118.0">
                    <children>
                        <Button layoutX="6.0" layoutY="189.0" mnemonicParsing="false" prefHeight="25.0" prefWidth="106.0" styleClass="buttons-new" text=" Settings" />
                        <ImageView fitHeight="18.0" fitWidth="18.0" layoutX="8.0" layoutY="193.0" pickOnBounds="true" preserveRatio="true">
                            <image>
                                <Image url="@setting.png" />
                            </image>
                        </ImageView>
                        <Button layoutX="6.0" layoutY="226.0" mnemonicParsing="false" prefHeight="25.0" prefWidth="106.0" styleClass="buttons-new" text="Help" />
                        <ImageView fitHeight="22.0" fitWidth="22.0" layoutX="6.0" layoutY="228.0" pickOnBounds="true" preserveRatio="true">
                            <image>
                                <Image url="@help.png" />
                            </image>
                        </ImageView>
                        <Button fx:id="Sign_out" layoutX="6.0" layoutY="261.0" mnemonicParsing="false" prefHeight="25.0" prefWidth="106.0" styleClass="buttons-new" text="Sign out" />
                        <ImageView fitHeight="18.0" fitWidth="18.0" layoutX="9.0" layoutY="265.0" pickOnBounds="true" preserveRatio="true">
                            <image>
                                <Image url="@logout.png" />
                            </image>
                        </ImageView>
                    </children>
                </AnchorPane>
            </children>
        </VBox>
        <AnchorPane fx:id="dashBoard" layoutX="118.0" layoutY="77.0" prefHeight="423.0" prefWidth="637.0">
         <children>
            <TableView layoutX="-2.0" layoutY="-1.0" prefHeight="423.0" prefWidth="700.0">
              <columns>
                <TableColumn prefWidth="85.0" text="Stock Name" />
                <TableColumn prefWidth="50.0" text="Type" />
                  <TableColumn prefWidth="75.0" text="Price 2020" />
                  <TableColumn prefWidth="70.0" text="Price 2021" />
                  <TableColumn prefWidth="70.0" text="Price 2022" />
                  <TableColumn prefWidth="95.0" text="Price 2023 Q1" />
                  <TableColumn prefWidth="95.0" text="Price 2023 Q2" />
                  <TableColumn prefWidth="95.0" text="Price 2023 Q3" />
                  <TableColumn prefWidth="65.0" text="Risk" />
              </columns>
            </TableView>
         </children></AnchorPane>
        <BorderPane fx:id="navBar" prefHeight="77.0" prefWidth="755.0">
            <left>
                <ImageView fx:id="logo" fitHeight="54.0" fitWidth="221.0" pickOnBounds="true" preserveRatio="true" BorderPane.alignment="CENTER">
                    <image>
                        <Image url="@white-removebg-preview.png" />
                    </image>
                </ImageView>
            </left>
            <right>
                <AnchorPane prefHeight="77.0" prefWidth="309.0" BorderPane.alignment="CENTER" />
            </right>
            <center>
                <AnchorPane prefHeight="200.0" prefWidth="200.0" BorderPane.alignment="CENTER">
                    <children>
                        <TextField fx:id="searchBar" layoutX="60.0" layoutY="26.0" prefHeight="26.0" prefWidth="309.0" promptText="Search" />
                        <Button fx:id="searchBarButton" layoutX="369.0" layoutY="26.0" mnemonicParsing="false" prefHeight="25.0" prefWidth="45.0" />
                        <ImageView fitHeight="18.0" fitWidth="18.0" layoutX="379.0" layoutY="30.0" pickOnBounds="true" preserveRatio="true">
                            <image>
                                <Image url="@search.png" />
                            </image>
                        </ImageView>
                    </children>
                </AnchorPane>
            </center></BorderPane>
    </children>
</AnchorPane>
